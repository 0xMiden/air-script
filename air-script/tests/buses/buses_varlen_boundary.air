def BusesAir

trace_columns {
    main: [a],
}

buses {
    multiset p,
    logup q,
}

public_inputs {
    inputs: [[2]],
}

boundary_constraints {
    enf p.first = null;
    enf q.first = null;
    enf p.last = inputs;
    enf q.last = inputs;
}

integrity_constraints {
    p.insert(1) when 1;
    p.remove(1) when 1;
    q.insert(1, 2) when 1;
    q.insert(1, 2) when 1;
    q.remove(1, 2) with 2;
}
