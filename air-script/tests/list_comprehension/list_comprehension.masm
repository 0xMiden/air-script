    padw mem_loadw.4294903304 drop drop
    # => [z_1, z_0, ...]
    # Exponentiate z trace_len times
    mem_load.4294903307 neg dup.0 neq.0
    while.true
        movdn.2 dup.1 dup.1 ext2mul
        # => [z_1^2, z_0^2, i, ...]
        movup.2 add.1 dup.0 neq.0
        # => [b, i+1, z_1^2, z_0^2, ...]
    end # END while
    push.0 mem_storew.500000100 # z^exp for trace_len
    # => [0, 0, z_1^2, z_0^2, ...]
    drop drop
    # => [z_1^2, z_0^2, num_of_cycles', -log2(trace_len), ...]
    # Clean stack
    dropw
